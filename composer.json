{
    "name": "antonioprimera/laravel-config-permissions",
    "description": "A simple package enabling the use of roles and permissions from a config file in a Laravel app.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Primera",
            "email": "antonio@cus.ro"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^6",
        "antonioprimera/lara-packager": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "AntonioPrimera\\ConfigPermissions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioPrimera\\ConfigPermissions\\Tests\\": "tests/"
        }
    },
    "require": {
        "spatie/laravel-package-tools": "^1.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AntonioPrimera\\ConfigPermissions\\ConfigPermissionsServiceProvider"
            ]
        }
    }
}